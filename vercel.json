{
  "cleanUrls": true,
  "rewrites": [
    {
      "source": "/dashboard",
      "destination": "/dashboard/index.html"
    },
    {
      "source": "/dashboard/",
      "destination": "/dashboard/index.html"
    },
    {
      "source": "/login",
      "destination": "/login/index.html"
    },
    {
      "source": "/login/",
      "destination": "/login/index.html"
    },
    {
      "source": "/signup",
      "destination": "/signup/index.html"
    },
    {
      "source": "/signup/",
      "destination": "/signup/index.html"
    },
    {
      "source": "/about",
      "destination": "/about.html"
    },
    {
      "source": "/about/",
      "destination": "/about.html"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
